<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title><%= current_page.data.title %></title>
    <meta name="description" content="<%= data.site.meta_description %>">
    <meta name="theme-color" content="#2E416A"/>
    <!-- Facebook Open Graph data -->
    <meta property="og:title" content="<%= data.site.meta_title %>" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://www.alexgwebdev.com" />
    <meta property="og:image" content="https://res.cloudinary.com/dcf4y5ngp/image/upload/f_auto,q_auto/v1543600470/meta2.png" />
    <meta property="og:description" content="<%= data.site.meta_description %>" />
    <meta property="og:site_name" content="<%= data.site.meta_title %>" />
    <!-- Twitter Card data -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="<%= data.site.twitter_account %>">
    <meta name="twitter:title" content="<%= data.site.meta_title %>">
    <meta name="twitter:description" content="<%= data.site.meta_description %>">
    <meta name="twitter:creator" content="<%= data.site.twitter_account %>">
    <meta name="twitter:image:src" content="https://res.cloudinary.com/dcf4y5ngp/image/upload/f_auto,q_auto/v1543600470/meta2.png">
    <%= stylesheet_link_tag "application" %>
    <script type="text/javascript" src="/tarteaucitron/tarteaucitron.js"></script>

    <script type="text/javascript">
      tarteaucitron.init({
        "privacyUrl": "/privacy.html",
        "hashtag": "#tarteaucitron",
        "cookieName": "tartaucitron",
        "orientation": "bottom",
        "showAlertSmall": false,
        "cookieslist": true,
        "adblocker": false,
        "AcceptAllCta" : true,
        "highPrivacy": false,
        "handleBrowserDNTRequest": false,
        "removeCredit": true,
        "moreInfoLink": true
      });
    </script>
  </head>
  <body data-spy="scroll" data-target="#navbar" data-offset="70" class="preload">
    <%= partial "layouts/navbar" %>
    <%= yield %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.19.1/TweenMax.min.js"></script>
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <%= auto_javascript_include_tag %>
    <script type="text/javascript">
      tarteaucitron.user.gtagUa = 'UA-130123645-1';
      tarteaucitron.user.analyticsAnonymizeIp = true;
      tarteaucitron.user.gtagMore = function () { };
      (tarteaucitron.job = tarteaucitron.job || []).push('gtag');
    </script>
  </body>
</html>

